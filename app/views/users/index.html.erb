<h1>All registered users</h1>
<table class="table table-bordered table-hover">
  <tr>
    <th>Username or email</th>
    <th>Horoscope</th>
  </tr>
  <% @users.each do |user| %>
    <tr>
      <td><%= user.email if user.name.nil? || user.name.empty? %></td>
      <td><%= link_to "Show", show_horoscope_path(user) %></td>
    </tr>
  <% end %>
</table>